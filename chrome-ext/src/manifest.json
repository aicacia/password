{
	"manifest_version": 2,
	"name": "Aicacia Password",
	"browser_action": {
		"default_icon": {
			"16": "images/icon-16x16.png",
			"32": "images/icon-32x32.png",
			"48": "images/icon-48x48.png",
			"128": "images/icon-128x128.png"
		},
		"default_popup": "index.html"
	},
	"background": {
		"scripts": ["background.ts"],
		"persistent": true
	},
	"icons": {
		"16": "images/icon-16x16.png",
		"32": "images/icon-32x32.png",
		"48": "images/icon-48x48.png",
		"128": "images/icon-128x128.png"
	},
	"permissions": ["activeTab", "tabs"]
}